<h1>edit page for boats</h1>



<%= form_for @boat do |f| %>
 <%= f.label :name %>
 <%= f.text_field :name %> <br>

 <%= f.label :amt_container %>
 <%= f.text_field :amt_container %> <br>

 <%= f.label :location %>
 <%= f.text_field :location %> <br>

 <br><br>
 <center class="centerImg"> <%= f.file_field :image %><br> </center>

 <%= f.submit %>
<% end %>
